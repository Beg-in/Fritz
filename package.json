    "commit": "git add .",
    "push": "git push",
    "setup": "npm install -g gulp bower foreman node-inspector",
    "debug-command": "node-debug -c -p 8082 index.js",
    "debug": "NODE_DEBUG=1 nf run npm run debug-command"

